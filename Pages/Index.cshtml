@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<h1 class="display-4">This is multi tenant example</h1>

@if (Model.TenantInfo is null)
{
    <h2>Select a tenant.</h2>
    <p>
        Tenant sample
    </p>
}
else
{
    <h3>@($"Hello World, {Model.TenantInfo.Id}-{Model.TenantInfo.Name}")</h3>
}